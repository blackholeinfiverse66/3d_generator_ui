{
  "name": "3d-design-generator",
  "version": "1.0.0",
  "description": "AI-powered 3D design generation platform with React and Three.js",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4",
    "cors": "^2.8.5",
    "express": "^4.18.2"
  },
  "optionalDependencies": {
    "three": "^0.158.0",
    "@react-three/fiber": "^8.15.11",
    "@react-three/drei": "^9.88.13"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "server": "node server.js",
    "install-3d": "npm install three @react-three/fiber @react-three/drei",
    "dev": "concurrently \"npm run server\" \"npm start\"",
    "build-mobile": "cd mobile && expo build:android"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^7.6.0"
  },
  "keywords": [
    "3d-design",
    "ai",
    "react",
    "threejs",
    "design-generation",
    "material-switching",
    "3d-preview"
  ],
  "author": "Yash",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/3d-design-generator"
  }
}